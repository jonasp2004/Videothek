<Application x:Class="Ui.Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Ui.Desktop"
             StartupUri="MainWindow.xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             d1p1:Ignorable="d"
             xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:Ui.Logic.ViewModel;assembly=Ui.Logic">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator"
                           d:IsDataSource="True" />

            <FontFamily x:Key="EncodeSans">pack://application:,,,/Fonts/#Encode Sans Condensed Medium</FontFamily>
            <FontFamily x:Key="EncodeSansBlack">pack://application:,,,/Fonts/#Encode Sans Condensed Black</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>